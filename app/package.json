{
  "name": "yieldi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "npm run lint:prettier && npm run lint:next && npm run lint:tsc",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:next:fix",
    "lint:next": "next lint",
    "lint:next:fix": "npm run lint:next -- --fix",
    "lint:prettier": "npx prettier --check .",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "lint:tsc": "tsc --noEmit",
    "openapi:gen": "npm run openapi:gen:midgard && npm run openapi:gen:thornode",
    "openapi:gen:midgard": "API_SPEC=midgard npx @hey-api/openapi-ts",
    "openapi:gen:thornode": "API_SPEC=thornode npx @hey-api/openapi-ts",
    "openapi:pull": "npm run openapi:pull:midgard && npm run openapi:pull:thornode",
    "openapi:pull:midgard": "curl https://midgard.ninerealms.com/v2/swagger.json | jq -S . > midgard-schema.json",
    "openapi:pull:thornode": "curl https://thornode.ninerealms.com/thorchain/doc/openapi.json | jq -S . > thornode-schema.json",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cosmjs/stargate": "^0.32.4",
    "@ledgerhq/hw-app-btc": "^10.4.3",
    "@ledgerhq/hw-app-cosmos": "^6.30.4",
    "@ledgerhq/hw-app-eth": "^6.40.3",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@ledgerhq/hw-transport-webusb": "^6.29.4",
    "@psf/bitcoincashjs-lib": "^4.0.3",
    "@reown/appkit": "^1.6.1",
    "@tanstack/react-query": "^5.59.0",
    "@walletconnect/universal-provider": "^2.17.3",
    "@xchainjs/xchain-bitcoin": "^1.2.0",
    "@xchainjs/xchain-bitcoincash": "^1.0.7",
    "@xchainjs/xchain-client": "^1.0.7",
    "@xchainjs/xchain-crypto": "^0.3.6",
    "@xchainjs/xchain-doge": "^1.0.7",
    "@xchainjs/xchain-litecoin": "^1.0.7",
    "@xchainjs/xchain-thorchain": "^2.0.10",
    "@xchainjs/xchain-util": "^1.0.6",
    "@xchainjs/xchain-utxo": "^1.0.7",
    "@xchainjs/xchain-utxo-providers": "^1.0.8",
    "bchaddrjs": "^0.5.2",
    "bignumber.js": "^9.1.2",
    "bitcoinjs-lib": "7.0.0-rc.0",
    "coininfo": "^5.2.1",
    "ethers": "5.6.9",
    "next": "14.2.13",
    "pino-pretty": "^13.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-number-format": "^5.4.2",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "tailwind-merge": "^2.5.4",
    "viem": "^2.21.29",
    "wif": "^5.0.0"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.4.2",
    "@hey-api/openapi-ts": "^0.53.11",
    "@ledgerhq/hw-transport-mocker": "^6.29.4",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "^1.8.8",
    "assert": "^2.1.0",
    "babel-loader": "^9.2.1",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "encoding": "^0.1.13",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "url": "^0.11.4"
  }
}
