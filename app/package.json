{
  "name": "yieldi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm run openapi:gen && next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "npm run lint:prettier && npm run lint:next && npm run lint:tsc",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:next:fix",
    "lint:next": "next lint",
    "lint:next:fix": "npm run lint:next -- --fix",
    "lint:prettier": "npx prettier --check .",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "lint:tsc": "tsc --noEmit",
    "openapi:gen": "npm run openapi:gen:midgard && npm run openapi:gen:thornode",
    "openapi:gen:midgard": "API_SPEC=midgard npx @hey-api/openapi-ts",
    "openapi:gen:thornode": "API_SPEC=thornode npx @hey-api/openapi-ts",
    "openapi:gen:stagenet": "npm run openapi:gen:midgard:stagenet && npm run openapi:gen:thornode:stagenet",
    "openapi:gen:midgard:stagenet": "IS_STAGENET=true API_SPEC=midgard npx @hey-api/openapi-ts",
    "openapi:gen:thornode:stagenet": "IS_STAGENET=true API_SPEC=thornode npx @hey-api/openapi-ts",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cosmjs/stargate": "^0.32.4",
    "@ledgerhq/hw-app-eth": "^6.40.3",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@ledgerhq/hw-transport-webusb": "^6.29.4",
    "@reown/appkit": "^1.6.1",
    "@tanstack/react-query": "^5.59.0",
    "@walletconnect/universal-provider": "^2.17.3",
    "@xchainjs/xchain-avax": "^1.0.12",
    "@xchainjs/xchain-bitcoin": "^1.2.0",
    "@xchainjs/xchain-bitcoincash": "^1.0.7",
    "@xchainjs/xchain-bsc": "^1.0.12",
    "@xchainjs/xchain-client": "^1.0.7",
    "@xchainjs/xchain-cosmos": "^2.0.11",
    "@xchainjs/xchain-doge": "^1.0.7",
    "@xchainjs/xchain-ethereum": "^1.0.12",
    "@xchainjs/xchain-litecoin": "^1.0.7",
    "@xchainjs/xchain-thorchain": "^2.0.10",
    "@xchainjs/xchain-util": "^1.0.6",
    "@xchainjs/xchain-utxo-providers": "^1.0.8",
    "bignumber.js": "^9.1.2",
    "bitcoinjs-lib": "7.0.0-rc.0",
    "clsx": "^2.1.1",
    "ethers": "5.6.9",
    "next": "14.2.13",
    "pino-pretty": "^13.0.0",
    "qrcode.react": "^4.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-number-format": "^5.4.2",
    "react-toastify": "^11.0.3",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "tailwind-merge": "^2.5.4",
    "viem": "^2.21.29"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.8.1",
    "@hey-api/openapi-ts": "^0.64.3",
    "@ledgerhq/hw-transport-mocker": "^6.29.4",
    "@sentry/cli": "^2.41.1",
    "@sentry/nextjs": "^8.54.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "babel-loader": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "url": "^0.11.4"
  }
}
